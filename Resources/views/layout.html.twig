{% block script %}
<script>
function StateRibbon (itemid,state) {
    switch(itemid) {
        default:
            alert(itemid);
    }
}

function ClickRibbon (itemid,state) {
    switch(itemid) {
        case 'menu_home':
            window.location = "{{ path('arii_Doc_index') }}";
            break;
        case 'menu_note':
            window.location = "{{ path('arii_Doc_notes') }}";
            break;            
        default:
            window.location = "{{ url("arii_Doc_index") }}edit/"+itemid;
    }
}
</script>
